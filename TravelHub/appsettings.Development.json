{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Identity": {
    "SignIn": {
      "RequireConfirmedAccount": "false"
    },
    "Password": {
      "RequiredLength": "8",
      "RequireDigit": "true",
      "RequireUppercase": "true",
      "RequireLowercase": "true",
      "RequireNonAlphanumeric": "false"
    }
  }
}
